<form action="/lend" method="post" enctype="multipart/form-data">
  WHAT ARE YOU LENDING?
  <br>
  <input type="text" name="item_name">
  <br>
  <input type="file" name="image_upload" id="input">
  <br>
  DESCRIBE IT!
  <br>
  <textarea name="item_description" rows="10" cols="30">
  </textarea>
  <br>
  <input type="submit" value="Submit">
  <br>
</form>
  <progress max="100" value="0" class="node-js" name="progress_bar" style="display:none;">
    <!-- Browsers that support HTML5 progress element will ignore the html inside `progress` element. Whereas older browsers will ignore the `progress` element and instead render the html inside it. -->
    <div class="progress-bar">
      <span style="width: 35%">35%</span>
    </div>
  </progress>   
  

